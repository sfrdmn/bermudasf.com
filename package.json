{
  "name": "bermudasf.com",
  "version": "0.1.0",
  "private": true,
  "description": "Jon Knox bagz",
  "keywords": [
    "jon",
    "knox",
    "bermuda sf",
    "jon knox",
    "love"
  ],
  "scripts": {
    "serve": "node index.js",
    "scss": "node-sass --source-map public/css/main.min.css.map --output-style compressed scss/main.scss public/css/main.min.css",
    "scss-dev": "node-sass scss/main.scss public/css/main.css",
    "watch": "node-sass --source-map public/css/main.css.map -w scss -o public/css main.css",
    "postinstall": "npm run scss-dev"
  },
  "author": "Sean Fridman <fridman@mail.sfsu.edu> (http://seanfridman.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/sfrdmn/bermudasf.com"
  },
  "dependencies": {
    "okcms": "git+ssh://git@github.com:okfocus/okcms.git#v0.1.3"
  },
  "devDependencies": {
    "node-sass": "^2.1.1"
  }
}
