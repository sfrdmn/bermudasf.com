<head>
	<meta charset="utf-8" />
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />


	<title><%= @getPreparedTitle() %></title>
  <link rel="icon"
      href="/favicon.ico">
	<meta name="description" content="<%= @getPreparedDescription() %>" />
	<meta name="keywords" content="<%= @getPreparedKeywords() %>" />
	<meta name="viewport" content="width=device-width" />

  <link type="text/css" rel="stylesheet" href="/static/css/main.css">

  <script type="text/javascript" src="//use.typekit.net/ovk1dzs.js"></script>
  <script type="text/javascript">try{Typekit.load();}catch(e){}</script>
  <script type="text/javascript" src="/static/vendor/js/modernizr.custom.min.js"></script>
</head>
<body>

  <%- @content %>

  <script type="text/javascript">
    Modernizr.load({
      test: Modernizr.touch,
      yep: [
        '/static/vendor/js/zepto.min.js'
      ],
      nope: [
       '//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js',
       '/static/vendor/js/jquery.scrollTo.min.js'
      ],
      complete: function() {
        if (!window['jQuery']) {
          Modernizr.load('/static/vendor/js/jquery.min.js');
        }
        Modernizr.load('/static/js/app.js');
      }
    });
  </script>

  <script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

    ga('create', 'UA-43792625-1', 'auto');
    ga('send', 'pageview');

  </script>
</body>
</html>
